{
  "name": "example-logger",
  "version": "1.0.0",
  "description": "Example plugin that logs all session lifecycle events",
  "author": "AI Encounters",
  "main": "index.js",
  "dependencies": [],
  "permissions": {
    "events": ["session:*"],
    "hooks": ["*"],
    "storage": false,
    "network": false
  },
  "config": {
    "logLevel": "info",
    "logToFile": true
  }
}
