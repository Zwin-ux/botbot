{
  "name": "actuary",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered encounters engine for games and applications",
  "scripts": {
    "build": "pnpm -r build",
    "build:tsc": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "dev": "pnpm -r --parallel dev"
  },
  "keywords": [
    "ai",
    "encounters",
    "game-engine",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "supertest": "^7.1.4",
    "typescript": "^5.3.0",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
