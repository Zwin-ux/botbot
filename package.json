{
  "name": "botbot",
  "version": "2.0.0",
  "description": "BotBot 2.0 - Agent core with plugin skills, vector memory, and embeddings",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "next dev",
    "build": "tsc --noEmit && next build",
    "start:next": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "cross-env NODE_ENV=test jest --coverage --watchAll=false --ci",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "prepare": "is-ci || husky install",
    "db:migrate": "node scripts/migrate-safe.js",
    "db:rollback": "node src/scripts/migrate.js --rollback",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:railway": "railway up",
    "health-check": "node scripts/health-check.js",
    "setup:db": "npm run db:migrate"
  },
  "keywords": [
    "discord",
    "bot",
    "productivity",
    "games",
    "natural-language"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.6.3",
    "@discordjs/rest": "^1.7.0",
    "date-fns": "^3.6.0",
    "debug": "^4.4.1",
    "discord-api-types": "^0.37.60",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "franc": "^5.0.0",
    "is-ci": "^4.1.0",
    "next": "^14.0.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "babel-jest": "^29.7.0",
    "chai": "^5.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sqlite": "^5.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
