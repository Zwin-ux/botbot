{
  "name": "botbot",
  "version": "1.0.0",
  "description": "A natural language Discord bot for team collaboration and games",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "prepare": "husky install",
    "db:migrate": "node src/scripts/migrate.js",
    "db:rollback": "node src/scripts/migrate.js --rollback"
  },
  "keywords": [
    "discord",
    "bot",
    "productivity",
    "games",
    "natural-language"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.6.3",
    "@discordjs/rest": "^1.7.0",
    "date-fns": "^3.6.0",
    "discord-api-types": "^0.37.60",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "node-cron": "^3.0.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.2.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
